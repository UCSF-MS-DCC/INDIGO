<div class="container">
  <div class="row">
    <div class="col-sm-4"></div>
    <div class="col-sm-4"><h2 class="text-center">Upload keyfile data</h2></div>
  </div>
  <div class="row">
    <div class="col-sm-12">
     <%= form_for @key, html: { multipart: true } do |f| %>
        <%= f.label :keyfile %>
        <%= f.file_field :keyfile %>
        <%= f.submit "Save", class: "btn btn-indigo" %>
     <% end %>
    </div>
  </div>
</div>
