<p id="notice"><%= notice %></p>

<h1>Upload Reports</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @upload_reports.each do |upload_report| %>
      <tr>
        <td><%= link_to 'Show', upload_report %></td>
        <td><%= link_to 'Edit', edit_upload_report_path(upload_report) %></td>
        <td><%= link_to 'Destroy', upload_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Upload Report', new_upload_report_path %>
