<p id="notice"><%= notice %></p>

<h1>Gwas</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gwas.each do |gwa| %>
      <tr>
        <td><%= link_to 'Show', gwa %></td>
        <td><%= link_to 'Edit', edit_gwa_path(gwa) %></td>
        <td><%= link_to 'Destroy', gwa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gwa', new_gwa_path %>
